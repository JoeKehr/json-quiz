{
  "id": "question-match",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "meta": {
      "$ref": "metadata"
    },
    "random": {
      "type": "boolean"
    },
    "objects": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "$ref": "content"
      }
    },
    "resources": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "$ref": "content"
      }
    },
    "firstSet": {
      "type": "array",
      "minItems": 2,
      "uniqueItems": true,
      "items": {
        "$ref": "content"
      }
    },
    "secondSet": {
      "type": "array",
      "minItems": 2,
      "uniqueItems": true,
      "items": {
        "$ref": "content"
      }
    },
    "solutions": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "object",
        "properties": {
          "firstId": {
            "type": "string"
          },
          "secondId": {
            "type": "string"
          },
          "score": {
            "type": "number"
          }
        },
        "required": ["firstId", "secondId", "score"],
        "additionalProperties": false
      }
    },
    "feedback": {
      "type": "string"
    }
  },
  "required": ["id",  "title", "random", "multiple", "firstSet", "secondSet"],
  "additionalProperties": false
}